### global keys ###

### default for all ##
IgnoreUnknown UseKeychain

Host *
     UseKeychain yes
     IdentityFile ~/.ssh/id_rsa
     AddKeysToAgent yes
     ForwardAgent yes
     LogLevel ERROR
     StrictHostKeyChecking no
     ServerAliveInterval 60
     ServerAliveCountMax 60

## galactica: VPN server ##
Host galactica
     Hostname 52.35.240.100
     User ubuntu
     IdentityFile ~/.ssh/id_aws.pem
     RequestTTY yes
#      RemoteCommand if [ -z "$TMUX" ] && [ "$SSH_CONNECTION" != "" ]; then tmux attach-session -t "$USER" || tmux new-session -s "$USER"; fi

## felcon: Office R7000 router ##
Host felcon
     Hostname 10.10.1.1
     User root

## orion: Lab R7000 router ##
Host orion
     Hostname 10.10.3.1
     User root

## yamato: Lab R7000 router ##
Host yamato
     Hostname 10.10.4.1
     User root

## enterprise: Windows Laptop ##
Host enterprise-felcon
     Hostname 10.10.1.136
     User yaozh

Host enterprise-orion
     Hostname 10.10.3.142
     User yaozh

Host enterprise-felcon-vpn
     Hostname 10.10.1.136
     User yaozh
     ProxyCommand ssh -W %h:%p felcon 2>/dev/null

Host enterprise-orion-vpn
     Hostname 10.10.3.142
     User yaozh
     IdentityFile ~/.ssh/id_ed25519
     ProxyCommand ssh -W %h:%p orion 2>/dev/null

## hammurabi: Linux Laptop ##
Host hammurabi-felcon
     Hostname 10.10.1.118
     User yao
     RequestTTY yes
     RemoteCommand if [ -z "$TMUX" ] && [ "$SSH_CONNECTION" != "" ]; then tmux attach-session -t "$USER" || tmux new-session -s "$USER"; fi

